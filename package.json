    "@babel/runtime-corejs2": "^7.26.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "babel-loader": "^9.2.1",
    "cross-env": "^5.2.1",
    "expect": "^29.7.0",
    "glob": "^7.2.3",
    "globals": "^15.14.0",
    "isparta-loader": "^2.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-cli": "^2.0.0",
